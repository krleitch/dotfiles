{
    "vim.insertModeKeyBindings": [
        {
            "before": ["j", "j"],
            "after": ["<esc>"]
        },
        {
            "before": ["C-e"],
            "after": ["C-o", "$"]
        },
        {
            "before": ["C-a"],
            "after": ["C-o", "^"]
        },
        {
            "before": ["C-h"],
            "after": ["<Left>"]
        },
        {
            "before": ["C-j"],
            "after": ["<Down>"]
        },
        {
            "before": ["C-k"],
            "after": ["<Up>"]
        },
        {
            "before": ["C-l"],
            "after": ["<Right>"]
        },
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": ["H"],
            "after": ["^"]
        },
        {
            "before": ["L"],
            "after": ["$"]
        }
    ],
    "scrolloff.scrolloff": 5,
}
